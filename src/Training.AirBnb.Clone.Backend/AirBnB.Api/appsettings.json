{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PasswordValidationSettings": {
    "RequireDigit": true,
    "RequireUppercase": true,
    "RequireLowercase": true,
    "RequireNonAlphanumeric": false,
    "MinimumLength": 8,
    "MaximumLength": 10
  },
  "NotificationSubscriberSettings": {
    "PrefetchCount": 10
  }
}